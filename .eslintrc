{
  "env": {
    "node": true
  },
  "extends": ["wcl-frontend"],
  "rules": {
    "react/button-has-type": "off",
    "import/no-cycle": "off",
    "react/jsx-props-no-spreading": "off",
    "react/jsx-fragments": "off",
    "no-case-declarations": "off",
    "import/extensions": "off",
    "no-shadow": "off",
    "no-alert": "error",
    "no-restricted-globals": "off"
  },
  "globals": {
    "importScripts": true,
    "Module": true,
    "self": true
  }
}
